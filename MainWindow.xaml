<Window x:Class="Yumagulov41.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Yumagulov41"
        mc:Ignorable="d"
        Title="ИП Юмагулов" Height="1080" Width="1100" FontFamily="Comic Sans MS" Icon="Res/logo.png">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="75"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="30"/>
        </Grid.RowDefinitions>
        <Image Source="Res/logo.png" HorizontalAlignment="Left" ></Image>
        <TextBlock Text="ООО 41 размер" FontSize="30" HorizontalAlignment="Center" VerticalAlignment="Center"/>
        <Frame Grid.Row="1" Name="MainFrame" ContentRendered="MainFrame_ContentRendered" NavigationUIVisibility="Hidden"/>
        <Button Content="Назад" Name="BtnBack"  HorizontalAlignment="Right" 
                Click="BtnBack_Click" />
    </Grid>
</Window>
